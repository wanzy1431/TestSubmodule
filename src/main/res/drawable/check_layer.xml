<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                android:color="@color/yellow_CC"
                android:width="0.5dp" />
            <solid android:color="@color/white" />
        </shape>

    </item>
    <item
        android:top="3dp"
        android:right="3dp"
        android:bottom="3dp"
        android:left="3dp">
        <shape android:shape="oval">
            <solid android:color="@color/yellow_CC" />
        </shape>

    </item>

</layer-list>